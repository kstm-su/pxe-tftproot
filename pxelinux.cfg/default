default menu.c32
prompt 0
timeout 100
ontimeout local

label local
	menu label MBR
	kernel chain.c32
	append hd0 0

menu begin Ubuntu
	include pxelinux.cfg/ubuntu.cfg
menu end

menu begin Debian
	include pxelinux.cfg/debian.cfg
menu end

menu begin CentOS
	include pxelinux.cfg/centos.cfg
menu end

label hdd
	menu label HDD Tool
	kernel memdisk
	initrd ../tools/dft.img
